{
  "name": "dev-env",
  "version": "1.0.0",
  "description": "Node development environment",
  "main": "app.js",
  "scripts": {
    "lint": "esw src --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "open:src": "DEBUG=app:* nodemon --exec babel-node ./src/app.js",
    "start-message": "DEBUG=app:* babel-node ./buildScripts/startMessege",
    "dev": "npm-run-all --parallel start-message open:src lint:watch test:watch",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "build-message": "DEBUG=app:* babel-node ./buildScripts/buildMessage",
    "prebuild": "npm-run-all clean-dist test lint build-message",
    "build": "babel src --out-dir dist",
    "start": "NODE_ENV=production node ./dist/app.js"
  },
  "author": "Juliana Miranda",
  "license": "MIT",
  "dependencies": {
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0"
  },
  "engines": {
    "debug": "^4.3.2",
    "node": "16.6.0",
    "npm": "7.30.3"
  },
  "keywords": [
    "node",
    "starter",
    "boilerplate"
  ],
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.9",
    "@babel/preset-env": "^7.14.9",
    "@babel/register": "^7.14.5",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-watch": "^7.0.0",
    "mocha": "^9.0.3",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5"
  }
}
